<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hexaware.appwithfb.AppWithFB" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AppWithFB</name>
    <description>
        A sample WorkQuikr application that responds to the deviceready event.
    </description>
    <author email="WorkQuikr@gmail.com" href="http://cordova.io">
        WorkQuikr Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <feature name="Battery">
        <param name="wp-package" value="Battery" />
    </feature>
    <feature name="Accelerometer">
        <param name="wp-package" value="Accelerometer" />
    </feature>
    <feature name="Compass">
        <param name="wp-package" value="Compass" />
    </feature>
    <feature name="Camera">
        <param name="wp-package" value="Camera" />
    </feature>
    <feature name="File">
        <param name="wp-package" value="File" />
    </feature>
    <feature name="Capture">
        <param name="wp-package" value="Capture" />
    </feature>
    <feature name="Media">
        <param name="wp-package" value="Media" />
    </feature>
    <feature name="FileTransfer">
        <param name="wp-package" value="FileTransfer" />
    </feature>
    <feature name="Notification">
        <param name="wp-package" value="Notification" />
    </feature>
    <feature name="Vibration">
        <param name="wp-package" value="Vibration" />
    </feature>
    <feature name="Contacts">
        <param name="wp-package" value="Contacts" />
    </feature>
    <feature name="Globalization">
        <param name="wp-package" value="Globalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="wp-package" value="SplashScreen" />
    </feature>
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
	
    <feature name="hex.local">
      <param name="wp-package" value="hex.local"/>
    </feature>
    <feature name="io.cordova.Common.HexPlugins">
      <param name="wp-package" value="Common.HexPlugins"/>
    </feature>
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
</widget>
